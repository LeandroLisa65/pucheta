    var dia = "";
    if (fd[1].length == 1) {
        dia = "0" + fd[1];
    } else {
        dia = fd[1];
    }
    var mes = "";
    if (fd[0].length == 1) {
        mes = "0" + fd[0];
    } else {
        mes = fd[0];
    }
    var fdf = dia +"/"+ mes +"/"+ fd[2];
    $('#P_FechaParteDiario').html(fdf);

    //#endregion

    grillaActividades(data);
    grillaAsistencia(data.lstAsistContratista);
    grillaIncidentes(data.lstIncidentes);
    grillaIncidentesHistorial(data.lstIncidentesHistorial);
